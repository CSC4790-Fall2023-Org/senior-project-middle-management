{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./views/App/App.tsx",
    "paths": {
      "*": [
        "@src/*"
      ],
      "tests": [
        "@tests/*"
      ],
      "@components/*": [
        "@src/components/*"
      ]
    },
    "jsx": "react",
    "types": [
      "jest",
      "react-native"
    ],
    "esModuleInterop": true,
  }
}